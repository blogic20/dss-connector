<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0" metadata-complete="false">

    <enterprise-beans>
        <session>
            <ejb-name>DssConfigServiceImpl</ejb-name>
            <env-entry>
                <env-entry-name>home.sts.endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://asoa-dev.it.ru:9080/LECM-STS/STS</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.sts.endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://stenddss.cryptopro.ru/aeroflotidp/Active.svc</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.signService.endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>https://stenddss.cryptopro.ru/aeroflotss/SignService.svc</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>verificationService.endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>https://stenddss.cryptopro.ru/VerificationService/service.svc</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.userManagementService.endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>https://stenddss.cryptopro.ru/aeroflotidp/UserManagement.svc</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.policyCache.ttlMinutes</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>10</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.certIssuerCA.id</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.operator.username</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>gcd_admin@rshbank.ru</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.certificate.url</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>file:///home/admin/DSS.cer</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>home.sts.keystore.url</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>file:///home/admin/LecmSTS.p12</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>home.sts.keystore.password</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>o9p0[-]=</env-entry-value>
            </env-entry>

            <env-entry>
                <env-entry-name>dss.mfa.confirmIssue</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmSignDocument</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmSignDocuments</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmDecryptDocument</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmCreateRequest</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmDeleteCertificate</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmChangePin</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmRenewCertificate</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmRevokeCertificate</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmHoldCertificate</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dss.mfa.confirmUnholdCertificate</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>

</ejb-jar>
